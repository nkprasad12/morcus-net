{
  "name": "morcus-net",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "MAIN=build npm run tsnp src/scripts/prod_build_steps.ts && npm run tsnp src/esbuild/server.esbuild.ts && mv node_modules/better-sqlite3/build/Release/better_sqlite3.node ./build/",
    "start": "node build/server.js",
    "format": "npx eslint . && npx prettier . --write && python -m black .",
    "format-check": "npx eslint . && npx prettier . --check && python -m black . --check",
    "setup-alatius": "python src/scripts/alatius.py",
    "test": "npm run ts-tests && npm run py-tests",
    "ts-tests__comments": "Experimentally, it is faster to limit the number of workers.",
    "ts-tests": "npx jest --maxWorkers=8 --testPathIgnorePatterns src/integration/*",
    "integration-tests": "npx playwright test src/integration/suites/",
    "py-tests": "python -m unittest discover",
    "py-coverage": "python -m coverage run -m unittest discover && coverage xml --omit **/test_*.py -o coverage/coverage.xml",
    "ts-coverage": "npx jest --coverage --maxWorkers=8 --testPathIgnorePatterns src/integration/* --coveragePathIgnorePatterns src/integration",
    "coverage": "npm run ts-coverage && npm run py-coverage",
    "ts-node": "NODE_ENV=production node --max-old-space-size=4096 --env-file=.env -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register",
    "tsnp": "NODE_ENV=production node --max-old-space-size=4096 --env-file=.env -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register --swc",
    "pytype": "python -m pytype src/py src/scripts",
    "server-p": "npm run ts-node src/web/nlp/processing_server.ts",
    "ts-driver": "npm run ts-node src/ts_driver.ts -- --allowUnusedLabels",
    "py-driver": "python -c \"import src.py.driver\"",
    "loc-count": "npm run ts-node src/scripts/count_loc.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkprasad12/morcus-net.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/nkprasad12/morcus-net/issues"
  },
  "homepage": "https://github.com/nkprasad12/morcus-net#readme",
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.7.0",
    "@emotion/serialize": "^1.1.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@mui/material": "^5.11.12",
    "@playwright/test": "^1.49.1",
    "@swc/core": "^1.10.14",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/argparse": "^2.0.13",
    "@types/better-sqlite3": "^7.6.4",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^22.13.1",
    "@types/node-cleanup": "^2.1.2",
    "@types/supertest": "^2.0.12",
    "@types/ttest": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "argparse": "^2.0.1",
    "beta-code-js": "^3.0.0",
    "better-sqlite3": "^11.3.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "esbuild": "^0.25.0",
    "esbuild-plugin-output-reset": "^2.0.1",
    "eslint": "^9.21.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^4.18.2",
    "fake-indexeddb": "^5.0.2",
    "fast-xml-parser": "^4.3.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb": "6.5",
    "node-cleanup": "^2.1.2",
    "preact": "^10.26.0",
    "prettier": "2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "ttest": "^4.0.0",
    "typescript": "^5.7.3"
  }
}
