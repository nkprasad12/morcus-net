{
  "name": "morcus-net",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "MAIN=build npm run tsnp src/scripts/prod_build_steps.ts && npm run tsnp src/esbuild/server.esbuild.ts && mv node_modules/better-sqlite3/build/Release/better_sqlite3.node ./build/",
    "start": "node build/server.js",
    "format": "npx eslint . && npx prettier . --write && python -m black .",
    "format-check": "npx eslint . && npx prettier . --check && python -m black . --check",
    "setup-alatius": "python src/scripts/alatius.py",
    "test": "npm run ts-tests && npm run py-tests",
    "ts-tests__comments": "Experimentally, it is faster to limit the number of workers.",
    "ts-tests": "npx jest --maxWorkers=4 --testPathIgnorePatterns src/integration/*",
    "integration-tests": "npx jest src/integration/all_functional",
    "integration-tests-dev": "REUSE_DEV=1 npx jest src/integration/all_functional",
    "integration-tests-docker": "FROM_DOCKER=1 npx jest src/integration/all_functional",
    "py-tests": "python -m unittest discover",
    "py-coverage": "python -m coverage run -m unittest discover && coverage xml --omit **/test_*.py -o coverage/coverage.xml",
    "ts-coverage": "npx jest --coverage --maxWorkers=4 --testPathIgnorePatterns src/integration/* --coveragePathIgnorePatterns src/integration",
    "coverage": "npm run ts-coverage && npm run py-coverage",
    "tsnl": "node -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register -r dotenv/config --transpile-only",
    "ts-node": "node --max-old-space-size=4096 -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register",
    "tsnp": "NODE_ENV=production node --max-old-space-size=4096 -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register --transpile-only",
    "pytype": "python -m pytype src/py src/scripts",
    "server-p": "npm run ts-node src/web/nlp/processing_server.ts",
    "ts-driver": "npm run ts-node src/ts_driver.ts -- --allowUnusedLabels",
    "py-driver": "python -c \"import src.py.driver\"",
    "loc-count": "npm run ts-node src/scripts/count_loc.ts",
    "scale": "heroku ps:scale web=1 -a morcus-net",
    "unscale": "heroku ps:scale web=0 -a morcus-net",
    "logs": "heroku logs -t -a morcus-net",
    "scale-staging": "heroku ps:scale web=1 -a morcus-net-dev",
    "unscale-staging": "heroku ps:scale web=0 -a morcus-net-dev",
    "logs-staging": "heroku logs -t -a morcus-net-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkprasad12/morcus-net.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/nkprasad12/morcus-net/issues"
  },
  "homepage": "https://github.com/nkprasad12/morcus-net#readme",
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.6.1",
    "@emotion/react": "^11.10.6",
    "@jest/globals": "^29.7.0",
    "@mui/material": "^5.11.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/argparse": "^2.0.13",
    "@types/better-sqlite3": "^7.6.4",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@types/node-cleanup": "^2.1.2",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "argparse": "^2.0.1",
    "beta-code-js": "^3.0.0",
    "better-sqlite3": "^8.4.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "esbuild": "^0.20.1",
    "esbuild-plugin-define": "^0.4.0",
    "esbuild-plugin-output-reset": "^2.0.1",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "fake-indexeddb": "^5.0.2",
    "fast-xml-parser": "^4.3.5",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb": "6.5",
    "node-cleanup": "^2.1.2",
    "node-fetch": "2.6.9",
    "npm": "^10.5.2",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.1.3"
  },
  "optionalDependencies": {
    "puppeteer": "^21.6.1"
  }
}
