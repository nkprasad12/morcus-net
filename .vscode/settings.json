{
  "python.formatting.provider": "black",
  "python.testing.unittestArgs": ["-v", "-s", ".", "-p", "test_*.py"],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true,
  "[python]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5e3f48",
    "activityBar.background": "#5e3f48",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#0f130d",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#5e3f48",
    "statusBar.background": "#402b31",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#5e3f48",
    "statusBarItem.remoteBackground": "#402b31",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#402b31",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#402b3199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#402B31"
  // "python.analysis.typeCheckingMode": "basic"
}
